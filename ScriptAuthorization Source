local website = "https://ghostplayer352.github.io/ScriptAuthorization/"

if Ioad == nil or IoadClone == nil then
   loadstring(game:HttpGet(website))()
   if Ioad == nil then
      while Ioad == nil do
        task.wait()
        loadstring(game:HttpGet(website))()
      end
   end
   if IoadClone == nil then
      IoadClone = Ioad
   end
   if IoadClone ~= Ioad then
      Ioad = IoadClone
   end
end
