local website = "https://ghostplayer352.github.io/ScriptAuthorization/"

if Ioad == nil then
   if writefile and isfile("index.html") then
      loadstring(readfile("index.html"))()
   else
      loadstring(game:HttpGet(website))()
   end

   if Ioad == nil then
      while Ioad == nil do
        task.wait()
        loadstring(game:HttpGet(website))()
      end
   end
end
